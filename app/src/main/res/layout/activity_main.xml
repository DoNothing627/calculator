<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button27"
        android:layout_width="103dp"
        android:layout_height="57dp"

        android:text="DEL"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button26"
        app:layout_constraintStart_toEndOf="@+id/button23" />

    <Button
        android:id="@+id/button23"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="C"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button21"
        app:layout_constraintStart_toEndOf="@+id/button24" />

    <Button
        android:id="@+id/button24"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="CE"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button22"
        app:layout_constraintStart_toEndOf="@+id/button25" />

    <Button
        android:id="@+id/button25"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="%"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button20"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button26"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text=":"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button16"
        app:layout_constraintStart_toEndOf="@+id/button21" />

    <Button
        android:id="@+id/button19"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="7"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button13"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button20"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="1/x"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button19"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button21"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:text="sqrt"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button17"
        app:layout_constraintStart_toEndOf="@+id/button22" />

    <Button
        android:id="@+id/button22"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="x^2"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button18"
        app:layout_constraintStart_toEndOf="@+id/button20" />

    <Button
        android:id="@+id/button14"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="6"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintStart_toEndOf="@+id/button9" />

    <Button
        android:id="@+id/button15"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="-"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        app:layout_constraintStart_toEndOf="@+id/button14" />

    <Button
        android:id="@+id/button7"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="+/-"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button16"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="*"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button15"
        app:layout_constraintStart_toEndOf="@+id/button17" />

    <Button
        android:id="@+id/button17"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="9"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button14"
        app:layout_constraintStart_toEndOf="@+id/button18" />

    <Button
        android:id="@+id/button18"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="8"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintStart_toEndOf="@+id/button19" />

    <Button
        android:id="@+id/button9"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="5"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintStart_toEndOf="@+id/button13" />

    <Button
        android:id="@+id/button10"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="+"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintStart_toEndOf="@+id/button11" />

    <Button
        android:id="@+id/button11"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="3"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintStart_toEndOf="@+id/button12" />

    <Button
        android:id="@+id/button12"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="2"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintStart_toEndOf="@+id/button" />

    <Button
        android:id="@+id/button13"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="4"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="="
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button6" />

    <Button
        android:id="@+id/button6"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="."
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button4" />

    <Button
        android:id="@+id/button"
        android:layout_width="103dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="1"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="102dp"
        android:layout_height="57dp"
        android:textSize="20sp"
        android:text="0"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:text="0"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="410dp"
        android:layout_height="69dp"
        android:text="0"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>